{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "webapp",
  "compatibility_date": "2026-01-18",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  // D1 Database binding
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "edge-control-db",
      "database_id": "local-only"
    }
  ]
}